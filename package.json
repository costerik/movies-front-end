{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "vite",
    "lint": "eslint . && npm run prettier:check",
    "lint:fix": "eslint . --fix && npm run prettier:fix",
    "prettier:check": "prettier --check .",
